<p id="notice"><%= notice %></p>

<h1>Sends</h1>

<table>
  <thead>
    <tr>
      <th>Subject</th>
      <th>Message</th>
      <th>Email</th>
      <th>Sender</th>
      <th>Sendtime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sends.each do |send| %>
      <tr>
        <td><%= send.subject %></td>
        <td><%= send.message %></td>
        <td><%= send.email %></td>
        <td><%= send.sender %></td>
        <td><%= send.sendtime %></td>
        <td><%= link_to 'Show', send %></td>
        <td><%= link_to 'Edit', edit_send_path(send) %></td>
        <td><%= link_to 'Destroy', send, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <td><%= link_to 'Send', send_email_send_path(send) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Send', new_send_path %>

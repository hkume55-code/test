<p id="notice"><%= notice %></p>

<h1>送信リスト</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Name</th>
      <th>TrackingCode</th>
      <th>SendFlag</th>
      <th>SendAt</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sends.each do |send| %>
      <tr>
        <td><%= send.email %></td>
        <td><%= send.name %></td>
        <td><%= send.tracking_code %></td>
        <td><%= send.send_flag %></td>
        <td><%= send.send_at %></td>
        <td><%= link_to 'Show', send %></td>
        <td><%= link_to 'Edit', edit_send_path(send) %></td>
        <td><%= link_to 'Destroy', send, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Send', new_send_path %>
<%= link_to 'Dashboard', dashboards_path %>
